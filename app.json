{
  "expo": {
    "name": "Njik-Provider",
    "slug": "rn-NijkProvider-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon3.png",
    "userInterfaceStyle": "light",
    "notifications": {
      "icon": "./app/assets/images/icon3.png"
    },
    "splash": {
      "image": "./app/assets/images/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.mrpharman.customer",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.mrpharman.customer",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/icon3.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./app/assets/images/icon3.png",
          "color": "#FFFFFF"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow App to use your location"
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/82d0aeae-8255-4ee8-b129-b32e752a8cce"
    },
    "extra": {
      "eas": {
        "projectId": "fa549242-f638-4378-bb35-69f9f6c849c9"
      }
    }
  }
}
